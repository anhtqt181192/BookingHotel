
@{
    ViewData["Title"] = "Danh Sách Phòng";
}

@section Css {
<style>
    body {
            background: url(@(ViewBag.Banner != null ? ViewBag.Banner.Link : "/img/slideshow_1.jpg")) no-repeat;
            background-size: 100% auto;

        }
</style>
<style>
        

        header {
            background: #fff;
        }

        .page-index-header {
            margin: 0px;
            padding: 25px 0px;
            text-align: center;
            background: rgba(245,245,245,0.8);
        }

            .page-index-header > h2 {
                font-size: 30px;
                color: #ec6841;
                letter-spacing: 0px;
                margin-top: 0;
                text-transform: uppercase;
                font-weight: 500;
                font-family: 'Roboto', sans-serif;
                text-shadow: 0px 0px 3px #b59472;
            }

        .background-blur {
            -webkit-filter: blur(5px);
            -moz-filter: blur(5px);
            -o-filter: blur(5px);
            -ms-filter: blur(5px);
            filter: blur(5px);
        }

        h3 {
            font-weight: 500;
    line-height: 25px;
    color: #ec6841;
    font-family: 'Roboto', sans-serif;
        }
    </style>

}

<div class="container-fluid p-0">
    <div class="background-blur"></div>
    <div class="mx-auto page-index-header">
        <h2>
            DANH SÁCH PHÒNG
        </h2>
    </div>
</div>


<partial name="_PartialRoom" />

